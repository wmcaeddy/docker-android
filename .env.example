# Railway Environment Variables
# Copy this file to .env and configure as needed

# Emulator Configuration
EMULATOR_DEVICE=Samsung Galaxy S10
EMULATOR_ANDROID_VERSION=11.0
EMULATOR_API_LEVEL=30

# Display Configuration
WEB_VNC=true
SCREEN_WIDTH=1600
SCREEN_HEIGHT=900
SCREEN_DEPTH=24

# Performance Settings
EMULATOR_NO_BOOT_ANIM=true
EMULATOR_ADDITIONAL_ARGS=-gpu swiftshader_indirect -no-audio -no-boot-anim

# Network Configuration (Railway will provide PORT)
VNC_PORT=5900
WEB_VNC_PORT=6080
ADB_PORT=5555

# Optional: Authentication
# VNC_PASSWORD=your_secure_password

# Optional: Proxy Configuration (if needed)
# HTTP_PROXY=
# HTTPS_PROXY=
# NO_PROXY=